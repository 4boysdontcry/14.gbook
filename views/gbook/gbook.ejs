<!-- views폴더의 모든 파일들은 화면을 구성하는 역할 -->
<%- include('../inc/head.ejs') %>
<%- include('../inc/header.ejs') %>
<link rel="stylesheet" href="/css/gbook/gbook.css">
<div class="wrapper">
    <%- include('./title.ejs') %>
    <%- include('./form.ejs') %>
    <table class="table table-hover text-center gbook-tb">
        <thead class="thead-dark">
            <tr>
                <th>번호</th>
                <th>작성자</th>
                <th>내용</th>
                <th>첨부</th>
                <th>작성일</th>
                <th>관리</th>
            </tr>
        </thead>
        <tbody>
            <% for(let v of lists) { %> 
            <%- include('./list', {v}) %>
            <% } %> 
        </tbody>
    </table>
</div>
<%- include('../inc/pager.ejs', {...pager}) %>      <!-- {...pager}로 보낸 값들이 pager.ejs에 전달되어 값이 들어간다. -->
<%- include('./modal.ejs') %>

<%- include('../inc/footer.ejs') %>
<script src="/js/gbook/gbook.js"></script>
<%- include('../inc/foot.ejs') %>
<tr>
    <td><%= v.id %></td>
    <td><%= v.writer %> </td>
    <td class="text-left"><%= v.content %></td>
    <td>
        <% if(v.savename) { %> 
        <img src="<%= v.savename %>" alt="첨부이미지" class="img" onclick="onImgModal(this)">
        <% } %>
    </td>
    <td><%= v.createdAt %></td>
    <% if(user){ %>
        <td>
            <% if(user.id === v.uid) { %>
                <button onclick="onUpdate('<%= v.id %>');" class="btn btn-sm btn-success">수정</button>      <!-- 삭제할 데이터의 아이디를 받아서 -->
                <button onclick="onDelete('<%= v.id %>');" class="btn btn-sm btn-danger">삭제</button>
            <% } %>
        </td>
    <% } %>
</tr>
<script src="/js/gbook/list.js"></script>